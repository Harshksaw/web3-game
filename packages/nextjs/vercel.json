{
  "buildCommand": "yarn build",
  "installCommand": "yarn install --frozen-lockfile",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "pages/**/*": {
      "runtime": "nodejs20.x"
    }
  },
  "build": {
    "env": {
      "YARN_ENABLE_IMMUTABLE_INSTALLS": "false",
      "ENABLE_EXPERIMENTAL_COREPACK": "1", 
      "VERCEL_TELEMETRY_DISABLED": "1"
    }
  },
  "env": {
    "NODE_VERSION": "20.18.3"
  }
}
